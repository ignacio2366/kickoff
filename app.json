{
  "expo": {
    "name": "kickoff",
    "slug": "kickoff",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/snikers-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.markangeloignacio001.kickoff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/snikers-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.markangeloignacio001.kickoff"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/snikers-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/snikers-logo.png",
          "imageWidth": 250,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "packagerOpts": {
      "config": "/metro.config.js"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1f31e001-09bd-4d22-9a56-023c3ffd7b47"
      }
    }
  }
}
